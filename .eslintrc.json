{
    "env": {
        "browser": true,
        "node": true, 
        "es2020": true
    },
    "extends": ["eslint:recommended", "airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": { 
        "no-unused-vars": "warn" ,
        "allowForLoopAfterthoughts": true 
    }
}
